{
    "name": "alexbowers/laravel-flow",
    "description": "",
    "keywords": [
        "Laravel",
        "Flow",
        "Queue",
        "Delay"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Bowers",
            "email": "bowersbros@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/contracts": "~5.6",
        "illuminate/queue": "~5.6",
        "illuminate/support": "~5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Flow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Flow\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\Flow\\FlowServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
